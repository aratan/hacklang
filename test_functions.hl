let add = |x, y| {
  x + y;
};

let resultado = add(30, 12);
print resultado;

let operar = |func, a, b| {
  func(a, b);
};

let resultado_op = operar(add, 100, 50);
print resultado_op;

let factorial = |n| {
  (n < 2) ? { 1 } : { n * factorial(n - 1) };
};

let fact_5 = factorial(5);
print fact_5;